[10.252736, "o", "                  |  _ < (_) | |  __/ | |_) | | | | | (_| | | | | | (_| \\__ \\         \r\n                  |_| \\_\\___/|_|\\___| |____/|_|_| |_|\\__,_|_|_| |_|\\__, |___/         \r\n                                                                   |___/              \r\n                                 ____                                                 \r\n                                |  _ \\  ___ _ __ ___   ___                            \r\n                                | | | |/ _ \\  _   _ \\ / _ \\                           \r\n                                | |_| |  __/ | | | | | (_) |                          \r\n                                |____/ \\___|_| |_| |_|\\___/                           \r\n"]
[10.253491, "o", "                                                                                      \r\n          ----------------------------------------------------------------------------\r\n                       Role Binding - Allow Privileged Containers                     \r\n                               by Sacha Dubois, VMware Inc                            \r\n          ----------------------------------------------------------------------------\r\n                                                                                      \r\n"]
[15.408097, "o", " 1.) To authenticate to Kubernetes API as 'Normal User' a X509 Client Certificate is requred\r\n"]
[15.408492, "o", "      - Create private key\r\n"]
[15.408637, "o", "      - Create CertificateSigningRequest\r\n"]
[15.408895, "o", "     \r\n"]
[15.480072, "o", "     => \u001b[1mopenssl genrsa -out /tmp/user1.key 2048\u001b(B\u001b[m"]
[16.420226, "o", "\r\n"]
[16.423915, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[16.904398, "o", "     Generating RSA private key, 2048 bit long modulus (2 primes)\r\n     ..........................................................+++++\r\n"]
[16.904608, "o", "     ....................+++++\r\n     e is 65537 (0x010001)\r\n"]
[16.908023, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[16.90811, "o", "\r\n"]
[16.975694, "o", "     => \u001b[1mopenssl req -new -key /tmp/user1.key -out /tmp/user1.csr -subj \"/CN=user1/O=group1/O=group2\"\u001b(B\u001b[m"]
[17.584788, "o", "\r\n"]
[17.593001, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[17.729782, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[17.731818, "o", " 2.) Create a CertificateSigningRequest\r\n"]
[17.73202, "o", "      - Create a CertificateSigningRequest and submit it to a Kubernetes Cluster via kubectl\r\n     \r\n"]
[17.791973, "o", "     => cat /tmp/csr.yaml"]
[18.642728, "o", "\r\n"]
[18.648107, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[18.841713, "o", "     \u001b[38;5;203mapiVersion\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186mcertificates.k8s.io/v1\u001b[0m\r\n"]
[18.841898, "o", "     \u001b[38;5;203mkind\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186mCertificateSigningRequest\u001b[0m\r\n     \u001b[38;5;203mmetadata\u001b[0m\u001b[38;5;231m:\u001b[0m\r\n"]
[18.841938, "o", "     \u001b[38;5;231m  \u001b[0m\u001b[38;5;203mname\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186muser1\u001b[0m\r\n"]
[18.842644, "o", "     \u001b[38;5;203mspec\u001b[0m\u001b[38;5;231m:\u001b[0m\r\n"]
[18.849731, "o", "     \u001b[38;5;231m  \u001b[0m\u001b[38;5;203mrequest\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;"]
[18.850037, "o", "\u001b[0m\r\n     \u001b[38;5;231m  \u001b[0m\u001b[38;5;203msignerName\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186mkubernetes.io/kube-apiserver-client\u001b[0m\r\n"]
[18.851106, "o", "     \u001b[38;5;231m  \u001b[0m\u001b[38;5;203musages\u001b[0m\u001b[38;5;231m:\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;231m[\u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;186mdigital signature\u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;231m,\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;186mkey encipherment\u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;231m,\u001b[0m\u001b[38;5;231m \u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;186mclient auth\u001b[0m\u001b[38;5;186m'\u001b[0m\u001b[38;5;231m]\u001b[0m\r\n"]
[18.859384, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[18.926555, "o", "     => \u001b[1mkubectl apply -f /tmp/csr.yaml\u001b(B\u001b[m"]
[19.636199, "o", "\r\n"]
[19.647083, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[22.700418, "o", "     certificatesigningrequest.certificates.k8s.io/user1 created\r\n"]
[22.704585, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[22.772742, "o", "     => \u001b[1mkubectl get csr\u001b(B\u001b[m"]
[24.418478, "o", "\r\n"]
[24.432764, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[26.130803, "o", "     NAME    AGE   SIGNERNAME                            REQUESTOR                         REQUESTEDDURATION   CONDITION\r\n"]
[26.132228, "o", "     user1   3s    kubernetes.io/kube-apiserver-client   sso:Administrator@vsphere.local   <none>              Pending\r\n"]
[26.136147, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[26.137077, "o", " 3.) The Kubernetes Administrator needs to Approve or Deny the CSR request\r\n"]
[26.204666, "o", "     => \u001b[1mkubectl certificate approve user1\u001b(B\u001b[m"]
[26.77925, "o", "\r\n"]
[26.786509, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[28.924834, "o", "     certificatesigningrequest.certificates.k8s.io/user1 approved\r\n"]
[28.927889, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[28.928515, "o", " 4.) Retreive the Certificate\r\n      - The certificate value is in Base64-encoded format under status.certificate.\r\n"]
[28.928761, "o", "     \r\n"]
[28.99627, "o", "     => \u001b[1mkubectl get csr/user1 -o yaml\u001b(B\u001b[m"]
[29.586964, "o", "\r\n"]
[29.589551, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[31.091262, "o", "     apiVersion: certificates.k8s.io/v1\r\n"]
[31.091438, "o", "     kind: CertificateSigningRequest\r\n     metadata:\r\n       annotations:\r\n         kubectl.kubernetes.io/last-applied-configuration: |\r\n"]
[31.09195, "o", "           {\"apiVersion\":\"certificates.k8s.io/v1\",\"kind\":\"CertificateSigningRequest\",\"metadata\":{\"annotations\":{},\"name\":\"user1\"},\"spec\":{\"request\":\""]
[31.092049, "o", "\",\"signerName\":\"kubernetes.io/kube-apiserver-client\",\"usages\":[\"digital signature\",\"key encipherment\",\"client auth\"]}}\r\n       creationTimestamp: \"2022-10-20T21:47:13Z\"\r\n       name: user1\r\n       resourceVersion: \"2453231\"\r\n       uid: c03a23c6-d6a6-4a92-918f-5acd1da47068\r\n     spec:\r\n       groups:\r\n       - sso:LicenseService.Administrators@vsphere.local\r\n       - sso:SystemConfiguration.ReadOnly@vsphere.local\r\n"]
[31.092981, "o", "       - sso:SystemConfiguration.SupportUsers@vsphere.local\r\n       - sso:Administrators@vsphere.local\r\n       - sso:Everyone@vsphere.local\r\n       - sso:CAAdmins@vsphere.local\r\n       - sso:SystemConfiguration.Administrators@vsphere.local\r\n       - sso:SystemConfiguration.BashShellAdministrators@vsphere.local\r\n       - sso:Users@vsphere.local\r\n       - system:authenticated\r\n       request: "]
[31.093062, "o", "\r\n       signerName: kubernetes.io/kube-apiserver-client\r\n       usages:\r\n       - digital signature\r\n       - key encipherment\r\n       - client auth\r\n       username: sso:Administrator@vsphere.local\r\n     status:\r\n       certificate: LS0tLS1CRUdJTiBDRVJUSUZJQ0FURS0tLS0tCk1JSURKVENDQWcyZ0F3SUJBZ0lRUUtZNDhpRnVNL25KRWozVmJENUpiekFOQmdrcWhraUc5dzBCQVFzRkFEQVYKTVJNd0VRWURWUVFERXdwcmRXSmxjbTVsZEdWek1CNFhEVEl5TVR"]
[31.093106, "o", ""]
[31.093603, "o", "\r\n       conditions:\r\n       - lastTransitionTime: \"2022-10-20T21:47:19Z\"\r\n         lastUpdateTime: \"2022-10-20T21:47:19Z\"\r\n         message: This CSR was approved by kubectl certificate approve.\r\n         reason: KubectlApprove\r\n         status: \"True\"\r\n         type: Approved\r\n"]
[31.096529, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[31.16554, "o", "     => \u001b[1mkubectl get csr user1 -o jsonpath='{.status.certificate}'| base64 -d > /tmp/user1.crt\u001b(B\u001b[m"]
[31.819156, "o", "\r\n"]
[31.819474, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[33.289964, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[33.291163, "o", " 5.) Create Role and RoleBinding for the User\r\n      - create a Role 'developer' for the user user1\r\n      - create a RoleBinding for basic commands\r\n     \r\n"]
[33.360068, "o", "     => \u001b[1mkubectl create role developer --verb=create --verb=get --verb=list --verb=update --verb=delete --resource=pods --resource=services\u001b(B\u001b[m"]
[34.132538, "o", "\r\n"]
[34.142701, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[35.751066, "o", "     role.rbac.authorization.k8s.io/developer created\r\n"]
[35.75414, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n\r\n"]
[35.820821, "o", "     => \u001b[1mkubectl create rolebinding developer-binding-user1 --role=developer --user=user1\u001b(B\u001b[m"]
[36.577851, "o", "\r\n"]
[36.589024, "o", "     -----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------\r\n"]
[37.983274, "o", "     rolebinding.rbac.authorization.k8s.io/developer-binding-user1 created\r\n"]
